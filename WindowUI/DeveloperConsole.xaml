<Window x:Class="TMP.NET.WindowUI.DeveloperConsole"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TMP.NET.WindowUI"
        mc:Ignorable="d"
        Title="DeveloperConsole" Height="500" Width="500"
        SizeToContent="WidthAndHeight"
        ResizeMode="CanMinimize"
        WindowStartupLocation="CenterScreen"
        Background="#0A2647"
        Icon="/TMP.NET;component/Resources/icon.ico">
    <Grid>
        <TextBox x:Name="tbConsole"  HorizontalAlignment="Center" Height="422" Margin="0,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="480"/>
        <Grid HorizontalAlignment="Center" Height="37" Margin="0,437,0,0" VerticalAlignment="Top" Width="480">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <TextBox x:Name="tbInput" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" VerticalAlignment="Center" Width="460"/>
        </Grid>

    </Grid>
</Window>
